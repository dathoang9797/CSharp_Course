@model Euro.Models.Team

<form method="post">
    <div class="row mb-1">
        <label class="col-2">Name</label>
        <div class="col-10">
            <input class="form-control" type="text" name="name" id="name" value="@Model.TeamName"/>
        </div>
    </div>
    <div>
        <button class="btn btn-primary">Save Change</button>
        <a href="/team" class="btn btn-outline-primary">Cancel</a>
    </div>
</form>