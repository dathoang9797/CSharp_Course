@model IEnumerable<WebApp.Models.Member>

<table>
    <thead>
    <tr>
        <th>Email</th>
        <th>Roles</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Email</td>
            <td>
                <a href="/member/roles/@item.MemberId">Roles</a>
            </td>
        </tr>
    }
    </tbody>
</table>