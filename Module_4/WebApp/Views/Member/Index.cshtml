@model IEnumerable<WebApp.Models.Member>

<table class="table table-bordered">
    <thead class="table-dark">
    <tr>
        <th>Given Name</th>
        <th>Surname</th>
        <th>Email</th>
        <th>Roles</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GivenName</td>
            <td>@item.SurName</td>
            <td>@item.Email</td>
            <td>
                <a href="/member/roles/@item.MemberId">Roles</a>
            </td>
        </tr>
    }
    </tbody>
</table>